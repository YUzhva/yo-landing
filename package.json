{
  "name": "yo-landing",
  "version": "0.0.1",
  "description": "React + Redux + redux-saga lightweight starter kit",
  "scripts": {
    "start": "set NODE_ENV=development && webpack-dev-server --mode development --config ./config/webpack/dev.config.js --open --inline --hot",
    "start:serve-static": "node ./config/serve-static.js",
    "build": "set NODE_ENV=production && webpack --mode production --config ./config/webpack/prod.config.js",
    "lint": "yarn lint:js && yarn lint:scss",
    "lint:js": "eslint --ext .js src/",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss"
  },
  "main": "index.js",
  "repository": "git@github.com:mainset/yo-landing.git",
  "author": "Yevhen Uzhva <yevhen.uzhva@gmail.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0",
    "yarn": "^1.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "redux-devtools-extension": "^2.13.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.6.0",
    "stylelint-config-sass-guidelines": "^5.2.0",
    "stylelint-declaration-use-variable": "^1.7.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "bootstrap": "^3.4.0",
    "classnames": "^2.2.6",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "http-proxy-middleware": "^0.19.0",
    "immutable": "^3.8.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "reselect": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  }
}
